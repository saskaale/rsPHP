/* CONST */
function constfunc(const &t)
{
    t = 23;
    s = function(const &v) {
        v = 23;
        assert(v == 20);
        d = function(const &e) {
            e = 23;
            assert(e == 20);
        };
        d(v);
        d(t);
        assert(v == 20);
        assert(t == 20);
    };
    s(t);
}
ab = 20;
constfunc(ab);
assert(ab == 20);
