function test(&n)
{
    n = n + 1;
    print n;
}

function test2(&n)
{
    print ++n;
}

function test3(&n)
{
    n += 1;
    n = n + 2;
}

a = 23;
test(a);
test(a);
print a;

print "B";

b = 23;
test2(b);
test2(b);
print b;

print "C";

c = 23;
test3(c);
print c;

s1 = "hello";
s2 = "world";
swap(s1, s2);
print s1 + s2;


/* CONST */
function constfunc(const &t)
{
    t = 23;
}
ab = 20;
constfunc(ab);
assert(ab == 20);
